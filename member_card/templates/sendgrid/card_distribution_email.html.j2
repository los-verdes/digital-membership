        <td>
          <table border="0" cellspacing="0" cellpadding="0">
            <tr>
              <td align="right" style="border-radius: 3px; display: flex; text-align: right;">
                {%- for button in buttons -%}
                {%- if button.img_src -%}
                <a href="{~~ button.href ~~}" target="_blank" alt="{~~ button.text ~~}">
                  <img src="{~~ button.img_src ~~}">
                </a>
                  {%- else -%}
                <a href="{~~ button.href ~~}" target="_blank" alt="{~~ button.text ~~}"
                   style="background-color:#000000; border-radius:10px; border-width:0px; color:#ffffff; display:inline-block; font-size:12px; font-weight:normal; letter-spacing:0px; line-height:normal; padding: 5px; padding-bottom: 2px; max-height: 25px; text-align:center; text-decoration:none; border: 2px outset #00b140; font-family:verdana,geneva,sans-serif;">
                  {~~ button.text ~~}
                </a>
                {%- endif -%}
                &nbsp;&nbsp;&nbsp;
                {%- endfor -%}
              </td>
            </tr>
          </table>
        </td>
      </tr>
    </table>
  </td>
</tr>
<!-- END BUTTON -->
{% endmacro %}



